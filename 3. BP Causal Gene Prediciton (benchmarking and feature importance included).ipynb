    "    print('Test set accuracy: {:.2f}'.format(model.score(X_test, Y_test)*100),  '%')\n",
    "    print(\"Best Parameters: \\n{}\\n\".format(model.best_params_))\n",
    "\n",
    "fig = plt.figure()\n",
    "fig.suptitle('Nested Cross-Validation Comparison')\n",
